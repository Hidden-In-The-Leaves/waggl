{
  "name": "waggl",
  "version": "1.0.0",
  "description": "Dog Social Media App",
  "main": "index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint",
    "dev": "webpack serve --open --port 8080",
    "build": "webpack --watch",
    "pg:init": "pg_ctl init -D waggl",
    "pg:createdb": "createdb -h 127.0.0.1 waggl",
    "pg:start": "pg_ctl -D waggl start",
    "pg:stop": "pg_ctl -D waggl stop",
    "start": "nodemon ./server/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Hidden-In-The-Leaves/waggl"
  },
  "license": "ISC",
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@mui/material": "^5.10.0",
    "axios": "^0.27.2",
    "css-loader": "^6.7.1",
    "date-fns": "^2.29.1",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "node-pg": "^1.0.1",
    "nodemon": "^2.0.19",
    "pg": "^8.7.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "style-loader": "^3.3.1",
    "styled-components": "^5.3.5",
    "zustand": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "babel-loader": "^8.2.5",
    "eslint": "^8.22.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.0",
    "nodemon": "^2.0.19",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.10.0",
    "wepback": "^1.0.0"
  }
}
